    "expo-build-properties": "~0.14.6",
    "expo-constants": "~17.1.4",
    "expo-font": "~13.3.0",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.6",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "^14.1.4",
    "expo-linking": "~7.1.4",
    "expo-media-library": "~17.1.6",
    "expo-router": "^5.0.3",
    "expo-splash-screen": "~0.30.7",
    "expo-sqlite": "~15.2.9",
